// Visit https://docs.keel.so/ for documentation on how to get started
model ExperienceHighlight {
    fields {
        index ID
        highlight Text
        experience Experience
    }
}

model Experience {
    fields {
        companyName Text @unique
        jobTitle Text
        startDate Timestamp
        endDate Timestamp?
        highlights ExperienceHighlight[]
    }

    actions {
        list getExperiences()
    }
}
